<!DOCTYPE html>
<html>
<head>
<script>
var n=parseInt(prompt("enter a number"));
document.write("<h1 align="center">'multiplication table'</h1>");
document.write("<table border=1 align='center' bgcolor='yellow'>");
for(i=1;i<=10;i++)
{
	document.write("<tr>");
	document.write("<td>"+n+"</td><td>*</td><td>"+i+"</td><td>=</td><td>"+(n*i)+"</td>");
	document.write("</tr>");	
}
document.write("</table>");
</script>
</head>
</html>
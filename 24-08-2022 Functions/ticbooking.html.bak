<!DOCTYPE html>
<html>
<head>
<script>
document.write("<h1> BOOKING SUMMARY</h1>");
var seats=parseInt(window.prompt("enter no of seats"));

var calculateDiscount=(seats)=>
{

	if(seats==1)
	document.write("amount per one ticket is 9");
	else if(seats==2)
	{
	var dis1=9-9*0.5;
	var dis2=9-9*0.7;
	document.write("amount payable is "+(dis1+dis2));
	}
	else if(seats==3)
	{
	var dis1=9-9*0.5;
	var dis2=9-9*0.7;
	var dis3=9-9*0.9;
	document.write("amount payable is "+(dis1+dis2+dis3));
	}
	else if(seats==4)
	{
	var dis1=9-9*0.05;
	var dis2=9-9*0.07;
	var dis3=9-9*0.09;
	var dis4=9-9*0.11;
	document.write("amount payable is "+(dis1+dis2+dis3+dis4));
	}
	else if(seats==4)
	{
	var dis1=9-9*0.05;
	var dis2=9-9*0.07;
	var dis3=9-9*0.09;
	var dis4=9-9*0.11;
	var dis5=9-9*0.13;
	document.write("amount payable is "+(dis1+dis2+dis3+dis4+dis5));
	}
	
	if(seats>5)
	{
	document.write("booking not done");
	}


}
calculateDiscount(seats);
</script>
</head>
